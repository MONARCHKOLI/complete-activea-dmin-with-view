    myLinktick.addEventListener('click', function (e) {
    e.preventDefault();
    phoneNumber.style.display = 'none';
    myLink.style.display = 'none';
    pinField.style.display = 'block';
    confirmLink.style.display = 'inline-block';
    document.getElementById("tickDiv").style.display = "none";
    document.getElementById("enterpintext").style.display = "none";
    });
    
    // Add a keyup event listener to the pin field to restrict it to 4 digits
    pinField.addEventListener('keyup', function (e) {
    const pinValue = pinField.value;
    // Only enable the confirm link if the pin field has exactly 4 digits
    if (pinValue.length === 4) {
        confirmLink.disabled = false;
        confirmLink.classList.add("blueBtn");
        confirmLink.classList.remove("greyBtn");
    } else {
        confirmLink.disabled = true;
        confirmLink.classList.remove("blueBtn");
        confirmLink.classList.add("greyBtn");
    }
    
    // Truncate the pin field to 4 digits
    if (pinValue.length > 4) {
        pinField.value = pinValue.slice(0, 4);
    }
    });
    
    
    
    
    
    var subscribeBtn = document.getElementsByClassName('subscribe_btn')[0];
    var confirmBtn = document.getElementsByClassName('confirm_btn')[0];
    var pinInput = document.getElementsByClassName('pincode')[0];
    var msisdnInput = document.getElementsByClassName('msisdn')[0];
    var errorTextPinInput = document.getElementsByClassName('error-below-input-pincode')[0];
    var wrong_msisdn_text = document.getElementsByClassName('wrong-msisdn')[0];
    var spinner = document.getElementsByClassName("lds-roller")[0];
    var content_id = document.getElementsByClassName('custom_html_holder')[0].getAttribute('data-content-id')
    var numPanel = document.getElementsByClassName('num-panel')[0];
  	
    var orderBtn = document.getElementById('order');
    confirmBtn.href = orderBtn.href;
  
    // When LP load again after performing he_redirect user should see confirm page
    // So, extracting msisdn from current url because this time url contains msisdn
    // according to docs provided by partner
    var obj = get_current_url_params();
  
  
    if (obj?.msisdn !== undefined && obj?.contract_id !== undefined && sessionStorage.getItem("show_pin") === null) {
        msisdnInput.style.display = 'block';
        wrong_msisdn_text.style.display = 'block';
        msisdnInput.value = obj?.msisdn;
        msisdnInput.readOnly = true;
        subscribeBtn.style.display = 'block';
        spinner.style.display = 'none';
    }
    else if (obj?.msisdn === undefined && obj?.contract_id === undefined) {
        let body = document.getElementsByClassName("container-sec")[0];
        body.style.display = "none";
        window.onpaint = he_url_with_invite_params();
    }
    else {
        window.location.href;
    }
  
    if (obj?.msisdn === undefined && obj?.contract_id !== undefined) {
        subscribeBtn.disabled = false;
        subscribeBtn.style.display = 'block';
        msisdnInput.style.display = 'block';
        spinner.style.display = 'none';
    }
    else {
        window.location.href;
    }
  
    if (obj?.msisdn !== undefined && obj?.contract_id !== undefined && sessionStorage.getItem("show_pin") !== null) {
        subscribeBtn.style.display = 'none';
        msisdnInput.style.display = 'none';
        numPanel.style.display = 'none';
        spinner.style.display = 'none';
        confirmBtn.style.display = 'block';
        pinInput.style.display = 'block';
        look_for_otp();
    }
    else {
        window.location.href;
    }
  
    subscribeBtn.addEventListener("click", function (e) {
        e.preventDefault();
        if (obj?.msisdn === undefined && obj?.contract_id !== undefined) {
            if (validate_msisdn()) {
                window.location.href = pin_generate_url_with_params("wifi");
            }
            else {
                errorTextPinInput.textContent = 'Please enter your valid mobile number.';
                errorTextPinInput.style.display = 'block';
                return;
            }
        }
        else if (obj?.msisdn !== undefined && obj?.contract_id !== undefined) {
            window.location.href = pin_generate_url_with_params("wap");
        }
        else {
            window.location.href = he_url_with_invite_params();
        }
    })
  
    confirmBtn.addEventListener("click", function (e) {
        e.preventDefault();
        if (pinInput.value.length != 4) {
            errorTextPinInput.textContent = 'PIN code must be 4 digits';
            errorTextPinInput.style.display = 'block';
            return;
        }
        window.location.href = trigger_url_with_pin_params();
    })
  
    wrong_msisdn_text.addEventListener("click", function (e) {
        e.preventDefault();
        wrong_msisdn_text.style.display = 'none';
        msisdnInput.readOnly = false;
        msisdnInput.value = "";
        msisdnInput.focus();
        var url = window.location.href;
        params_string = url.split('?')[1];
        params = new URLSearchParams(params_string);
        params_obj = Object.fromEntries(params.entries());
        window.history.replaceState({}, "", url.split('?')[0] + '?contract_id=' + params_obj["contract_id"]);
    })
  
    // On first click user should redirect to he_redirect trigger 
    // with invite/order params required to create contract
    function he_url_with_invite_params() {
        sessionStorage.removeItem("show_pin");
        var url = "https://wires.orsedd.com/wires/Ae1playWire/he_redirect?trace=true&"
        var params_string = orderBtn.href.split("?")[1];
        var current_url = window.location.href;
        params_string = params_string + '&' + current_url.split('?')[1];
        window.location.replace(url.concat(params_string));
    }
  
    function trigger_url_with_pin_params() {
        var url = "https://wires.orsedd.com/wires/Ae1playWire/invite?trace=true&"
        var pin = pinInput.value;
        var params_string = orderBtn.href.split("?")[1];
        return url.concat(params_string + '&pin=' + pin + '&content_id=' + content_id);
    }
  
    function pin_generate_url_with_params(tag) {
        sessionStorage.setItem("show_pin", true);
        var url = `https://wires.orsedd.com/wires/Ae1playWire/generate_pin?trace=true&tag=${tag}&`
        var msisdn = '971' + parseInt(msisdnInput.value).toString().slice(-9);
        if(tag === 'wap')
		{
          msisdn = msisdnInput.value;
        }
        var params_string = orderBtn.href.split("?")[1];
        return url.concat(params_string + '&msisdn=' + msisdn);
    }
  
    function get_current_url_params() {
        var url = window.location.href;
        params_string = url.split('?')[1];
        params = new URLSearchParams(params_string);
        return Object.fromEntries(params.entries());
    }
  
    function validate_msisdn() {
        numberedMsisdn = parseInt(msisdnInput.value.replace(/[^\d]/g, '')).toString();
        
        if (numberedMsisdn.startsWith('971')){
            numberedMsisdn = numberedMsisdn.slice(-9);
        }

        return (numberedMsisdn.length === 9);
    }
