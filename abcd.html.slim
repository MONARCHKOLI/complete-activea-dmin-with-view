    <script>
    function validatePhoneNumber() {
    const phoneNumber = document.getElementById("phoneNumber").value;
    const phoneNumberLength = phoneNumber.length;
    
    if (phoneNumber.startsWith("05") && phoneNumberLength !== 10) {
        document.getElementById("errorDiv").innerHTML =
            "Please enter 8 digits after 05";
        document.getElementById("tickDiv").style.display = "none";
        document.getElementById("phoneNumber").disabled = false;
        document.getElementById("submitBtn").classList.add("greyBtn");
        document.getElementById("submitBtn").classList.remove("blueBtn");
    }
    
    else if (phoneNumber.startsWith("05") && phoneNumberLength === 10) {
        document.getElementById("submitBtn").classList.add("blueBtn");
        document.getElementById("submitBtn").classList.remove("greyBtn");
        document.getElementById("phoneNumber").disabled = true;
    }
    
    else if (phoneNumber.startsWith("5") && phoneNumberLength !== 9) {
        document.getElementById("errorDiv").innerHTML =
            "Please enter 8 digits after 5";
        document.getElementById("tickDiv").style.display = "none";
        document.getElementById("phoneNumber").disabled = false;
        document.getElementById("submitBtn").classList.add("greyBtn");
        document.getElementById("submitBtn").classList.remove("blueBtn");
    } 
    
    else if (phoneNumber.startsWith("5") && phoneNumberLength === 9) {
        document.getElementById("submitBtn").classList.add("blueBtn");
        document.getElementById("submitBtn").classList.remove("greyBtn");
        document.getElementById("phoneNumber").disabled = true;
    }
    else if (phoneNumber.startsWith("971") && phoneNumberLength !== 12) {
        document.getElementById("errorDiv").innerHTML =
            "Please enter 9 digits after 971";
        document.getElementById("tickDiv").style.display = "none";
        document.getElementById("phoneNumber").disabled = false;
        document.getElementById("submitBtn").classList.add("greyBtn");
        document.getElementById("submitBtn").classList.remove("blueBtn");
    }
    else if (phoneNumber.startsWith("971") && phoneNumberLength === 12) {
        document.getElementById("submitBtn").classList.add("blueBtn");
        document.getElementById("submitBtn").classList.remove("greyBtn");
        document.getElementById("phoneNumber").disabled = true;
    }
    
    
    else if (phoneNumber.startsWith("0971") && phoneNumberLength !== 14) {
        document.getElementById("errorDiv").innerHTML =
            "Please enter 10 digits after 0971";
        document.getElementById("tickDiv").style.display = "none";
        document.getElementById("phoneNumber").disabled = false;
        document.getElementById("submitBtn").classList.add("greyBtn");
        document.getElementById("submitBtn").classList.remove("blueBtn");
    } 
    else if (phoneNumber.startsWith("0971") && phoneNumberLength === 14) {
        document.getElementById("submitBtn").classList.add("blueBtn");
        document.getElementById("submitBtn").classList.remove("greyBtn");
        document.getElementById("phoneNumber").disabled = true;
    }
    
    else if (
        !phoneNumber.startsWith("05") &&
        !phoneNumber.startsWith("5") &&
        !phoneNumber.startsWith("971") &&
        !phoneNumber.startsWith("0971") && !phoneNumber.startsWith("097105") && !phoneNumber.startsWith("09715")
        && !phoneNumber.startsWith("9715") && !phoneNumber.startsWith("97105")
    ) {
        document.getElementById("errorDiv").innerHTML =
            "Invalid phone number format. Please enter a valid phone number starting with 05, 5, 971, 0971, 97105, 09715, 9715 or 097105";
        document.getElementById("tickDiv").style.display = "none";
        document.getElementById("phoneNumber").disabled = false;
        document.getElementById("submitBtn").classList.add("greyBtn");
        document.getElementById("submitBtn").classList.remove("blueBtn");
    } else {
        document.getElementById("errorDiv").innerHTML = "";
        document.getElementById("tickDiv").style.display = "inline-block";
        document.getElementById("phoneNumber").disabled = true;
        document.getElementById("submitBtn").classList.remove("greyBtn");
        document.getElementById("submitBtn").classList.add("blueBtn");
    }
    }
    
    const myLink = document.getElementById('submitBtn');
    const myLinktick = document.getElementById('tickDiv');
    const phoneNumber = document.getElementById('phoneNumber');
    const pinField = document.getElementById('pinField');
    const confirmLink = document.getElementById('confirmLink');
    
    myLink.addEventListener('click', function (e) {
    e.preventDefault();
    phoneNumber.style.display = 'none';
    myLink.style.display = 'none';
    pinField.style.display = 'block';
